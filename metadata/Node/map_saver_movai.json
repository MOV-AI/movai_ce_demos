{
    "Node": {
        "map_saver_movai": {
            "Info": "Node that converts an occupancy grid into a map (png + yaml) and save the same in the database.",
            "Label": "map_saver_movai",
            "LastUpdate": {
                "date": "19/10/2021 at 12:04:09",
                "user": "movai"
            },
            "Launch": true,
            "PackageDepends": "",
            "Parameter": {
                "filepath": {
                    "Description": "[string] Path of where to save the map file. \nIf empty, the map will not be saved into any file, only database.",
                    "Value": "\"\""
                },
                "map_name": {
                    "Description": "[string] Name of the map to be saved",
                    "Value": "saved_map"
                },
                "threshold_free": {
                    "Description": "[int] Threshold value to consider a cell free",
                    "Value": "25"
                },
                "threshold_occupied": {
                    "Description": "[int] Threshold value to consider a cell occupied",
                    "Value": "65"
                }
            },
            "Path": "",
            "Persistent": false,
            "PortsInst": {
                "entry": {
                    "In": {
                        "in": {
                            "Callback": "place_holder",
                            "Message": "movai_msgs/Transition"
                        }
                    },
                    "Message": "Transition",
                    "Package": "movai_msgs",
                    "Template": "MovAI/TransitionTo"
                },
                "init": {
                    "In": {
                        "in": {
                            "Callback": "map_saver_movai_init",
                            "Message": "movai_msgs/Init"
                        }
                    },
                    "Message": "Init",
                    "Package": "movai_msgs",
                    "Template": "MovAI/Init"
                },
                "map": {
                    "In": {
                        "in": {
                            "Callback": "map_save_map",
                            "Message": "nav_msgs/OccupancyGrid"
                        }
                    },
                    "Message": "OccupancyGrid",
                    "Package": "nav_msgs",
                    "Template": "ROS1/Subscriber"
                },
                "save_map": {
                    "In": {
                        "in": {
                            "Callback": "map_saver_movai_save_map",
                            "Message": "std_msgs/Empty"
                        }
                    },
                    "Message": "Empty",
                    "Package": "std_msgs",
                    "Template": "ROS1/Subscriber"
                }
            },
            "Remappable": true,
            "Type": "MovAI/State",
            "User": "",
            "Version": "",
            "VersionDelta": {}
        }
    }
}