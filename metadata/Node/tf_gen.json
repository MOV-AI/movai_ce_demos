{
    "Node": {
        "tf_gen": {
            "Info": "The tf gen (generator) node receives the tags_detections and the tag TF generated by the apriltag_ros node and publishes a new generated TF that is shifted in the z direction (distance parameter) and the x direction (shift parameter) of the tag TF.  This node is specific to the Tugbot robot model and how the camera is positioned in the model.  ",
            "Label": "tf_gen",
            "LastUpdate": {
                "date": "28/11/2021 at 20:53:24",
                "user": "movai"
            },
            "Launch": true,
            "PackageDepends": "",
            "Parameter": {
                "apriltag_tf_suffix": {
                    "Description": "The suffix of the TF generated by the apriltag_ros node",
                    "Type": "string",
                    "Value": "tag_"
                },
                "camera": {
                    "Description": "The camera of the tugbot model which is used to generate the tag TF. This parameter is used to rotated the generated TF according to the positioning of the front or back camera",
                    "Type": "string",
                    "Value": "back"
                },
                "distance": {
                    "Description": "The distance of the generated TF from the tag TF in the z direction of the tag TF",
                    "Type": "number",
                    "Value": "0.75"
                },
                "expected_tag_id": {
                    "Description": "The expected TF frame name that is generated using the apriltag_ros node ",
                    "Type": "string",
                    "Value": "0"
                },
                "generated_tf_frame": {
                    "Description": "Name of the generated TF frame",
                    "Type": "string",
                    "Value": "tf_generated"
                },
                "shift": {
                    "Description": "The distance of the generated TF from the tag TF in the x direction of the tag TF",
                    "Type": "number",
                    "Value": "0"
                }
            },
            "Path": "",
            "Persistent": false,
            "PortsInst": {
                "enable": {
                    "In": {
                        "in": {
                            "Callback": "tf_gen_enable",
                            "Message": "std_msgs/Bool"
                        }
                    },
                    "Message": "Bool",
                    "Package": "std_msgs",
                    "Template": "ROS1/Subscriber"
                },
                "generated_tf": {
                    "Message": "TF",
                    "Out": {
                        "out": {
                            "Message": "movai_msgs/TF",
                            "Parameter": {
                                "Child": "from",
                                "Parent": "to"
                            }
                        }
                    },
                    "Package": "movai_msgs",
                    "Template": "ROS1/TFPublisher"
                },
                "init": {
                    "In": {
                        "in": {
                            "Callback": "tf_gen_init",
                            "Message": "movai_msgs/Init"
                        }
                    },
                    "Message": "Init",
                    "Package": "movai_msgs",
                    "Template": "MovAI/Init"
                },
                "tag_detections": {
                    "In": {
                        "in": {
                            "Callback": "tf_gen_tag_detections",
                            "Message": "apriltag_ros/AprilTagDetectionArray"
                        }
                    },
                    "Message": "AprilTagDetectionArray",
                    "Package": "apriltag_ros",
                    "Template": "ROS1/Subscriber"
                },
                "tag_tf_sub": {
                    "In": {
                        "in": {
                            "Callback": "tf_gen_tag_tf_sub",
                            "Message": "movai_msgs/TF",
                            "Parameter": {
                                "Child": "tag_0",
                                "Frequency": 20,
                                "Parent": "base_link"
                            }
                        }
                    },
                    "Message": "TF",
                    "Package": "movai_msgs",
                    "Template": "ROS1/TFSubscriber"
                }
            },
            "Remappable": true,
            "Type": "MovAI/Node",
            "User": "",
            "Version": "",
            "VersionDelta": {}
        }
    }
}